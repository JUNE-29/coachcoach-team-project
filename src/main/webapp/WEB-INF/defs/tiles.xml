<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>
    <definition name="coachPage/*/*" template="/WEB-INF/tiles/coachPage/template.jsp">
         
        <put-attribute name="side" value="/WEB-INF/tiles/coachPage/side.jsp" />
        <put-attribute name="body" value="/WEB-INF/views/coachPage/{1}/{2}.jsp" />
        <put-attribute name="footer" value="/WEB-INF/tiles/coachPage/footer.jsp" />
    </definition>
    
    
    <definition name="myPage/*/*" template="/WEB-INF/tiles/myPage/template.jsp">
         
        <put-attribute name="side" value="/WEB-INF/tiles/myPage/side.jsp" />
        <put-attribute name="body" value="/WEB-INF/views/myPage/{1}/{2}.jsp" />
        <put-attribute name="footer" value="/WEB-INF/tiles/myPage/footer.jsp" />
    </definition>
        
    <definition name="auth/*/*" template="/WEB-INF/tiles/auth/template.jsp">
        <put-attribute name="header" value="/WEB-INF/tiles/auth/header.jsp" />
        <put-attribute name="body" value="/WEB-INF/views/auth/{1}/{2}.jsp" />
        <put-attribute name="footer" value="/WEB-INF/tiles/auth/footer.jsp" />
    </definition>
    
    <definition name="auth/*" template="/WEB-INF/tiles/auth/template.jsp">
        <put-attribute name="header" value="/WEB-INF/tiles/auth/header.jsp" />
        <put-attribute name="body" value="/WEB-INF/views/auth/{1}.jsp" />
        <put-attribute name="footer" value="/WEB-INF/tiles/auth/footer.jsp" />
    </definition>
    
    
    
    <definition name="myCoach/*/*" template="/WEB-INF/tiles/myCoach/template.jsp">
        <put-attribute name="header" value="/WEB-INF/tiles/myCoach/header.jsp" />
        <put-attribute name="side" value="/WEB-INF/tiles/myCoach/side.jsp" />
        <put-attribute name="body" value="/WEB-INF/views/myCoach/{1}/{2}.jsp" />
        <put-attribute name="footer" value="/WEB-INF/tiles/myCoach/footer.jsp" />
        <put-attribute name="jsFilename" value="{1}/{2}.js" />
    </definition>    
    
      <definition name="myCoach/program/reviewForm" template="/WEB-INF/tiles/myCoach/template2.jsp">
        <put-attribute name="body" value="/WEB-INF/views/myCoach/program/reviewForm.jsp" />
    </definition>    
    
     <definition name="program/*" template="/WEB-INF/tiles/program/pTemplate.jsp">
        <put-attribute name="header" value="/WEB-INF/tiles/program/header.jsp" />
        <put-attribute name="body" value="/WEB-INF/views/program/{1}.jsp" />
        <put-attribute name="footer" value="/WEB-INF/tiles/program/footer.jsp" />
    </definition> 
        
     <definition name="program/apply/*" template="/WEB-INF/tiles/program/template.jsp">
        <put-attribute name="header" value="/WEB-INF/tiles/program/header.jsp" />
        <put-attribute name="body" value="/WEB-INF/views/program/apply/{1}.jsp" />
        <put-attribute name="footer" value="/WEB-INF/tiles/program/footer.jsp" />
    </definition>    
    
   <definition name="serviceCenter/*" template="/WEB-INF/tiles/program/template2.jsp">
        <put-attribute name="header" value="/WEB-INF/tiles/serviceCenter/header.jsp" />
        <put-attribute name="body" value="/WEB-INF/views/serviceCenter/{1}.jsp" />
        <put-attribute name="footer" value="/WEB-INF/tiles/serviceCenter/footer.jsp" />
    </definition>    
    
</tiles-definitions>


